# Production Environment Configuration
# Use this configuration for production deployment

# Environment Mode
NODE_ENV=production
ENABLE_MOCK_FALLBACK=false

# Supabase Configuration (replace with your production values)
VITE_SUPABASE_URL=your_production_supabase_url
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_production_supabase_service_role_key

# Dify Integration
VITE_ENABLE_DIFY_INTEGRATION=true

# Dify API Configuration (replace with your production values)
VITE_DIFY_API_URL=https://api.dify.ai/v1
VITE_DIFY_APP_ID=your_production_app_id
VITE_DIFY_API_KEY=your_production_api_key

# Dify Timeout Configuration
VITE_DIFY_TIMEOUT_MS=30000
VITE_DIFY_WORKFLOW_TIMEOUT_MS=120000
VITE_DIFY_MAX_RETRIES=3

# Stripe Configuration (replace with your production values)
VITE_STRIPE_PUBLISHABLE_KEY=your_production_stripe_key
STRIPE_SECRET_KEY=your_production_stripe_secret

# Additional production configurations
# Add any other environment variables your production deployment needs